@import '/static/iconfont/iconfont';

.c-spec{
    height: 70rpx;
    padding: 0 20rpx;
    line-height: 70rpx;
    background: #fff;
    border-top: 1rpx solid #f1f1f1;
    border-bottom: 1rpx solid #f1f1f1;
}
.c-spec .title{
    float: left;
    color: #333;
    font-size: 28rpx;
}
.c-spec .iconfont{
    float: right;
    color: #888;
    font-size: 24rpx;
}