.c-picturew{
    overflow: hidden;
}
.c-picturew .list{
    overflow: hidden;
}
.c-picturew .item{
    width: 25%;
    float: left;
    text-align: center;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.c-picturew .item .img{
    width: 100%;
    height: 100%;
}
.c-picturew .list.row2 .item{
    width: 50%;
}
.c-picturew .list.row3 .item{
    width: 33.33%;
}

.c-picturew .list.win .box{
    width: 50%;
    float: left;
}
.c-picturew .win .item{
    width: 100%;
}
.c-picturew .win .last .item:first-child{
    width: 375rpx;
}
.c-picturew .win .last .item{
    width: 187.5rpx;
}
.c-picturew .win .img{
    height: 375rpx;
}
.c-picturew .win .last .item .img{
    height: 187.5rpx;
}