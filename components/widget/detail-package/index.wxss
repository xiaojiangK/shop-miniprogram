@import '/static/iconfont/iconfont';

.c-package{
    padding: 0 20rpx;
    background: #fff;
    border-top: 1rpx solid #f1f1f1;
    border-bottom: 1rpx solid #f1f1f1;
}
.c-package>.title{
    height: 76rpx;
    font-size: 28rpx;
    overflow: hidden;
    line-height: 76rpx;
    border-bottom: 1rpx solid #eee;
}
.c-package .title .left{
    float: left;
}
.c-package .title .right{
    float: right;
}
.c-package .title .iconfont{
    color: #888;
    font-size: 24rpx;
    margin: 0 0 0 8rpx;
}
.c-package .list{
    padding: 20rpx 20rpx 40rpx;
    overflow: hidden;
}
.c-package .list .item{
    width: 50%;
    float: left;
    padding: 0 10rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.c-package .item .img{
    width: 100%;
    height: 334rpx;
}
.c-package .item .title{
    color: #333;
    font-size: 24rpx;
    line-height: 40rpx;
}