.c-banner{
    height: 390rpx;
    position: relative;
}
.c-banner .swiper{
    height: 390rpx;
}
.c-banner .swiper .link{
    width: 100%;
    height: 390rpx;
}
.c-banner .swiper .img{
    width: 100%;
    height: 100%;
}
.c-banner .dots {
    width: 156rpx;
    height: 36rpx;
    display: flex;
    flex-direction: row;
    position: absolute;
    left: 320rpx;
    bottom: 20rpx;
}
.c-banner .dot {
    opacity: .8;
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    margin-right: 20rpx;
    background-color: white;
}
.c-banner .dots .active {
    opacity: 1;
}
.c-banner .dot.square{
    border-radius: 0;
}
.c-banner .dot.rectangle{
    width: 32rpx;
    border-radius: 0;
}